#include <fltk/RepeatButton.H>

MODULE = FLTK               PACKAGE = FLTK::RepeatButton

RepeatButton *
new(char *package, int XPos, int YPos, int Width, int Height, const char *Label = "")
    CODE:
        RETVAL = new RepeatButton(XPos, YPos, Width, Height, Label);
        RETVAL->copy_label(Label);
    OUTPUT:
        RETVAL
