#include <fltk/HighlightButton.H>

MODULE = FLTK               PACKAGE = FLTK::HighlightButton

HighlightButton *
new(char *package, int XPos, int YPos, int Width, int Height, const char *Label = "")
    CODE:
        RETVAL = new HighlightButton(XPos, YPos, Width, Height, Label);
        RETVAL->copy_label(Label);
    OUTPUT:
        RETVAL
